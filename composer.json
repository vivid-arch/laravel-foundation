{
    "name": "vivid-arch/laravel-foundation",
    "description": "The Foundation of the Vivid Architecture for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Meletios Flevarakis",
            "email": "foss@flevarakis.xyz",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Vivid\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vivid\\Foundation\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vivid\\Foundation\\VividServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5",
        "nunomaduro/larastan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.2"
    }
}
